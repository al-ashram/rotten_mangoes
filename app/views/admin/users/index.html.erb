<h1>Admin User Management</h1>



<%= link_to "Bak to All movies", movies_path %><br/>

<%= paginate @users %>

<table>
  <tr>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Admin?</th>
  <tr>
  <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><%= u.firstname %></td>
      <td><%= u.lastname %></td>
      <td><%= u.email %></td>
      <% if u.admin == true %>
        <td>Yes</td>
      <% else %>
        <td>No</td>
      <% end %>
    </tr>
  <% end %>
</table>
